<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="Optus GDL Checks" location="${POS_CHECKSTYLE}/pos-gdl-checks-no-suppressions.xml" type="external" description="Checkstyle GDL configuration files.">
    <additional-data name="cache-file" value="false"/>
    <additional-data name="protect-config-file" value="true"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="Optus GDL Checks" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
</fileset-config>
